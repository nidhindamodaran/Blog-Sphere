<%= link_to 'New Article', new_article_path %>
<table>
  <tr>
    <th>
      Title
    </th>
    <th>
      Text
    </th>
  </tr>
  <% @articles.each do|article| %>
    <tr>
      <td>
        <%= article.title %>
      </td>
      <td>
        <%= article.text %>
      </td>
    </tr>
  <% end %>

</table>
